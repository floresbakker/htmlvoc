    dct:created '2024-04-07'^^xsd:date;
    dct:source 'https://dom.spec.whatwg.org/'^^xsd:anyURI;
    skos:prefLabel 'Document Object Model (DOM) - Living Standard'@en;
    rdfs:isDefinedBy dom:.

