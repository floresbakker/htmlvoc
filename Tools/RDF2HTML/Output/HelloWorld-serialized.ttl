doc:1 a html:Document ;
    rdf:_1 doc:docType ;
    rdf:_2 doc:html ;
    html:fragment "<!DOCTYPE html><html><head><title>My first 'Hello world' HTML page represented in RDF</title><style> .italic-text {font-style: italic;}</style><meta charset=\"utf-8\" http-equiv=\"Content-Type\"></head><body>Hello world</body></html>"^^rdf:HTML .

<http://www.w3.org/DOM/id/section/DOMImplementation> a dcm:Text ;
    dct:isPartOf <http://www.w3.org/DOM/id/standard/dom> ;
    dct:source "https://dom.spec.whatwg.org/#interface-domimplementation"^^xsd:anyURI ;
    rdfs:isDefinedBy dom: ;
    skos:prefLabel "4.5.1. Interface DOMImplementation"@en .

<http://www.w3.org/DOM/id/section/NamedNodeMap> a dcm:Text ;
    dct:isPartOf <http://www.w3.org/DOM/id/standard/dom> ;
    dct:source "https://dom.spec.whatwg.org/#interface-namednodemap"^^xsd:anyURI ;
    rdfs:isDefinedBy dom: ;
    skos:prefLabel "4.9.1. Interface NamedNodeMap"@en .

<http://www.w3.org/DOM/id/section/NodeTree> a dcm:Text ;
    dct:isPartOf <http://www.w3.org/DOM/id/standard/dom> ;
    dct:source "https://dom.spec.whatwg.org/#node-trees"^^xsd:anyURI ;
    rdfs:isDefinedBy dom: ;
    skos:prefLabel "4.2. Node tree"@en .

dom:ProcessingInstruction a owl:Class ;
    dct:conformsTo <http://www.w3.org/DOM/id/section/ProcessingInstruction> ;
    rdfs:isDefinedBy dom: ;
    rdfs:subClassOf dom:CharacterData ;
    skos:definition "Represents a processing instruction in the DOM document. Processing instructions provide instructions to the application for processing the document."@en ;
    skos:prefLabel "Processing Instruction"@en .

dom:ShadowRoot a owl:Class ;
    dct:conformsTo <http://www.w3.org/DOM/id/section/ShadowRoot> ;
    rdfs:isDefinedBy dom: ;
    rdfs:subClassOf dom:DocumentFragment ;
    skos:definition "Represents a separate subtree of the document tree that can be associated with a particular element, effectively creating a 'shadow DOM' within the element."@en ;
    skos:prefLabel "Shadow Root"@en .

dom:class a owl:DatatypeProperty,
        dom:Attribute ;
    dct:conformsTo <http://www.w3.org/DOM/id/section/Class> ;
    rdfs:domain dom:Element ;
    rdfs:isDefinedBy dom: ;
    rdfs:range xsd:string ;
    skos:definition "Specifies a class for a DOM element."@en ;
    skos:prefLabel "the class attribute"@en ;
    dom:key "class" .

dom:customElementDefinition a owl:DatatypeProperty ;
    dct:conformsTo <http://www.w3.org/DOM/id/section/Element> ;
    rdfs:domain dom:Element ;
    rdfs:isDefinedBy dom: ;
    rdfs:range xsd:string ;
    skos:definition "Specifies a custom element definition for a DOM element."@en ;
    skos:prefLabel "custom element definition"@en .

dom:customElementState a owl:DatatypeProperty ;
    dct:conformsTo <http://www.w3.org/DOM/id/section/Element> ;
    rdfs:domain dom:Element ;
    rdfs:isDefinedBy dom: ;
    rdfs:range xsd:string ;
    skos:definition "Specifies a custom element state for a DOM element."@en ;
    skos:prefLabel "custom element state"@en .

dom:documentBaseURL a owl:DatatypeProperty ;
    dct:conformsTo <http://www.w3.org/DOM/id/section/Node> ;
    rdfs:domain dom:Document ;
    rdfs:isDefinedBy dom: ;
    rdfs:range xsd:string ;
    skos:definition "Specifies the node document's document base URL."@en ;
    skos:prefLabel "document base URL"@en .

dom:id a owl:DatatypeProperty,
        dom:Attribute ;
    dct:conformsTo <http://www.w3.org/DOM/id/section/ID> ;
    rdfs:domain dom:Element ;
    rdfs:isDefinedBy dom: ;
    rdfs:range xsd:string ;
    skos:definition "Specifies an associated unique identifier (ID) for a DOM element."@en ;
    skos:prefLabel "the id attribute"@en ;
    dom:key "id" .

dom:isValue a owl:DatatypeProperty ;
    dct:conformsTo <http://www.w3.org/DOM/id/section/Element> ;
    rdfs:domain dom:Element ;
    rdfs:isDefinedBy dom: ;
    rdfs:range xsd:string ;
    skos:definition "Specifies an is value for a DOM element."@en ;
    skos:prefLabel "is value"@en .

dom:name a owl:DatatypeProperty,
        dom:Attribute ;
    dct:conformsTo <http://www.w3.org/DOM/id/section/Name> ;
    rdfs:domain dom:Element ;
    rdfs:isDefinedBy dom: ;
    rdfs:range xsd:string ;
    skos:definition "Specifies a name for a DOM element."@en ;
    skos:prefLabel "the name attribute"@en .

dom:namespace a owl:DatatypeProperty ;
    dct:conformsTo <http://www.w3.org/DOM/id/section/Element> ;
    rdfs:domain dom:Element ;
    rdfs:isDefinedBy dom: ;
    rdfs:range xsd:anyUri ;
    skos:definition "Specifies the namespace for a DOM element."@en ;
    skos:prefLabel "namespace"@en .

dom:namespacePrefix a owl:DatatypeProperty ;
    dct:conformsTo <http://www.w3.org/DOM/id/section/Element> ;
    rdfs:domain dom:Element ;
    rdfs:isDefinedBy dom: ;
    rdfs:range xsd:string ;
    skos:definition "Specifies the namespace prefix for a DOM element."@en ;
    skos:prefLabel "namespace prefix"@en .

dom:nodeName a owl:DatatypeProperty ;
    dct:conformsTo <http://www.w3.org/DOM/id/section/Node> ;
    rdfs:domain dom:Node ;
    rdfs:isDefinedBy dom: ;
    rdfs:range xsd:string ;
    skos:definition "Specifies a name for a DOM node."@en ;
    skos:prefLabel "node name"@en .

dom:slot a owl:DatatypeProperty,
        dom:Attribute ;
    dct:conformsTo <http://www.w3.org/DOM/id/section/Slot> ;
    rdfs:domain dom:Element ;
    rdfs:isDefinedBy dom: ;
    rdfs:range xsd:string ;
    skos:definition "Specifies a slot for a DOM element."@en ;
    skos:prefLabel "the slot attribute"@en ;
    dom:key "slot" .

section:13.1.2 a dcm:Text ;
    dct:isPartOf standard:html ;
    dct:source "https://html.spec.whatwg.org/#elements-2"^^xsd:anyURI ;
    rdfs:isDefinedBy html: ;
    skos:definition "A section in the living standard of HTML."@en ;
    skos:prefLabel "§ 13.1.2 Elements"@en .

section:13.1.2-VoidElements a dcm:Text ;
    dct:isPartOf standard:html ;
    dct:source "https://html.spec.whatwg.org/#void-elements"^^xsd:anyURI ;
    rdfs:isDefinedBy html: ;
    skos:definition "A section in the living standard of HTML."@en ;
    skos:prefLabel "§ 13.1.2 Void elements"@en .

section:13.3 a dcm:Text ;
    dct:isPartOf standard:html ;
    dct:source "https://html.spec.whatwg.org/#serialising-html-fragments"^^xsd:anyURI ;
    rdfs:isDefinedBy html: ;
    skos:definition "A section in the living standard of HTML."@en ;
    skos:prefLabel "§ 13.3 Serializing HTML fragments"@en .

section:3.2.5.3 a dcm:Text ;
    dct:isPartOf standard:html ;
    dct:source "https://html.spec.whatwg.org/#transparent-content-models"^^xsd:anyURI ;
    rdfs:isDefinedBy html: ;
    skos:definition "A section in the living standard of HTML."@en ;
    skos:prefLabel "§ 3.2.5.3 Transparant content models"@en .

section:8.1.8 a dcm:Text ;
    dct:isPartOf standard:html ;
    dct:source "https://html.spec.whatwg.org/#events"^^xsd:anyURI ;
    rdfs:isDefinedBy html: ;
    skos:definition "A section in the living standard of HTML."@en ;
    skos:prefLabel "§ 8.1.8 Events"@en .

html:AutocapitalizeInheritingElement a owl:Class ;
    dct:conformsTo section:4.10.2 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:FormAssociatedElement ;
    skos:definition "Denotes elements that inherit the autocapitalize attribute from their form owner."@en ;
    skos:prefLabel "autocapitalize-inheriting element"@en .

html:AutonomousCustomElement a owl:Class ;
    dct:conformsTo section:4.13.3 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:CustomElement ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty html:extends ;
            owl:someValuesFrom [ owl:intersectionOf ( html:Element [ owl:complementOf [ owl:unionOf ( html:CustomizedBuiltInElement html:EscapableRawTextElement html:ForeignElement html:NormalElement html:RawTextElement html:VoidElement ) ] ] ) ] ] ;
    skos:definition "An autonomous custom element is a custom HTML element that is defined with no extends option. These types of custom elements have a local name equal to their defined name."@en ;
    skos:prefLabel "autonomous custom element"@en .

html:Caption a owl:Class ;
    dct:conformsTo section:4.9.2 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:NormalElement ;
    skos:definition "The caption element represents the title of the table that is its parent, if it has a parent and that is a table element."@en ;
    skos:prefLabel "the caption element"@en ;
    html:tag "caption" .

html:CustomDataAttribute a owl:Class ;
    dct:conformsTo section:3.2.6 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:Attribute ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:onDatatype xsd:string ;
                    owl:withRestrictions ( [ xsd:pattern "^data-[a-z_:][a-z0-9_.:-]*$" ] ) ] ;
            owl:onProperty html:key ] ;
    skos:definition "A custom data attribute is an attribute in no namespace whose name starts with the string \"data-\", has at least one character after the hyphen, is XML-compatible, and contains no ASCII upper alphas."@en ;
    skos:prefLabel "custom data attribute"@en .

html:EmbeddedContent a owl:Class ;
    dct:conformsTo section:3.2.5.2.6 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:ContentCategory ;
    owl:unionOf ( html:Audio html:Canvas html:Embed html:Iframe html:Img html:MathML html:Object html:SVG html:Video ) ;
    skos:definition "Embedded content is content that imports another resource into the document, or content from another vocabulary that is inserted into the document."@en ;
    skos:prefLabel "Embedded content"@en .

html:Figcaption a owl:Class ;
    dct:conformsTo section:4.4.13 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:NormalElement ;
    skos:definition "The <figcaption> HTML element represents a caption or legend describing the rest of the contents of its parent <figure> element."@en ;
    skos:prefLabel "the figcaption element"@en ;
    html:tag "figcaption" .

html:FlowContent a owl:Class ;
    dct:conformsTo section:3.2.5.2.2 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:ContentCategory ;
    owl:unionOf ( [ owl:intersectionOf ( html:Area [ a owl:Restriction ;
                            owl:onProperty [ owl:inverseOf rdfs:member ] ;
                            owl:someValuesFrom html:Map ] ) ] [ owl:unionOf ( [ owl:intersectionOf ( html:Link [ a owl:Restriction ;
                                        owl:cardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onProperty html:itemprop ] ) ] [ owl:intersectionOf ( html:Link [ owl:unionOf ( [ a owl:Restriction ;
                                                    owl:hasValue "dns-prefetch" ;
                                                    owl:onProperty html:rel ] [ a owl:Restriction ;
                                                    owl:hasValue "modulepreload" ;
                                                    owl:onProperty html:rel ] [ a owl:Restriction ;
                                                    owl:hasValue "pingback" ;
                                                    owl:onProperty html:rel ] [ a owl:Restriction ;
                                                    owl:hasValue "preconnect" ;
                                                    owl:onProperty html:rel ] [ a owl:Restriction ;
                                                    owl:hasValue "prefetch" ;
                                                    owl:onProperty html:rel ] [ a owl:Restriction ;
                                                    owl:hasValue "preload" ;
                                                    owl:onProperty html:rel ] [ a owl:Restriction ;
                                                    owl:hasValue "stylesheet" ;
                                                    owl:onProperty html:rel ] ) ] ) ] ) ] [ owl:intersectionOf ( html:Meta [ a owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty html:itemprop ] ) ] [ owl:intersectionOf ( html:Main [ owl:unionOf ( [ a owl:Restriction ;
                                        owl:onProperty [ owl:inverseOf rdfs:member ] ;
                                        owl:someValuesFrom html:Html ] [ a owl:Restriction ;
                                        owl:onProperty [ owl:inverseOf rdfs:member ] ;
                                        owl:someValuesFrom html:Body ] [ a owl:Restriction ;
                                        owl:onProperty [ owl:inverseOf rdfs:member ] ;
                                        owl:someValuesFrom html:Div ] [ a owl:Restriction ;
                                        owl:onProperty [ owl:inverseOf rdfs:member ] ;
                                        owl:someValuesFrom html:CustomElement ] [ a owl:Restriction ;
                                        owl:onProperty [ owl:inverseOf rdfs:member ] ;
                                        owl:someValuesFrom [ owl:intersectionOf ( html:Form [ owl:complementOf [ a owl:Restriction ;
                                                                    owl:cardinality "1"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty aria:aria-label ] ] ) ] ] ) ] ) ] html:A html:Abbr html:Address html:Article html:Aside html:Audio html:B html:Bdi html:Bdo html:Blockquote html:Br html:Button html:Canvas html:Cite html:Code html:Data html:Datalist html:Del html:Details html:Dfn html:Dialog html:Div html:Dl html:Em html:Embed html:Fieldset html:Figure html:Footer html:Form html:H1 html:H2 html:H3 html:H4 html:H5 html:H6 html:Header html:Hgroup html:Hr html:I html:Iframe html:Img html:Input html:Ins html:Kbd html:Label html:Map html:Mark html:MathML html:Menu html:Meter html:Nav html:Noscript html:Object html:Ol html:Output html:P html:Picture html:Pre html:Progress html:Q html:Ruby html:S html:Samp html:Script html:Search html:Section html:Select html:Slot html:Small html:Span html:Strong html:Sub html:Sup html:SVG html:Table html:Template html:Textarea html:Time html:U html:Ul html:Var html:Video html:Wbr html:CustomElement html:Text ) ;
    skos:definition "Most elements that are used in the body of documents and applications are categorized as flow content."@en ;
    skos:prefLabel "Flow content"@en .

html:HeadingContent a owl:Class ;
    dct:conformsTo section:3.2.5.2.4 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:ContentCategory ;
    owl:unionOf ( html:H1 html:H2 html:H3 html:H4 html:H5 html:H6 [ owl:intersectionOf ( html:Hgroup [ a owl:Restriction ;
                            owl:onProperty rdfs:member ;
                            owl:someValuesFrom [ owl:unionOf ( html:H1 html:H2 html:H3 html:H4 html:H5 html:H6 ) ] ] ) ] ) ;
    skos:definition "Heading content defines the heading of a section (whether explicitly marked up using sectioning content elements, or implied by the heading content itself)."@en ;
    skos:prefLabel "Heading content"@en .

html:InteractiveContent a owl:Class ;
    dct:conformsTo section:3.2.5.2.7 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:ContentCategory ;
    owl:unionOf ( [ owl:intersectionOf ( html:A [ a owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty html:href ] ) ] [ owl:intersectionOf ( html:Audio [ a owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty html:controls ] ) ] [ owl:intersectionOf ( html:Img [ a owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty html:usemap ] ) ] [ owl:intersectionOf ( html:Input [ owl:complementOf [ a owl:Restriction ;
                                    owl:hasValue "hidden" ;
                                    owl:onProperty html:type ] ] ) ] [ owl:intersectionOf ( html:Video [ a owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty html:controls ] ) ] html:Button html:Details html:Embed html:Iframe html:Label html:Select html:Textarea ) ;
    skos:definition "Interactive content is content that is specifically intended for user interaction."@en ;
    skos:prefLabel "Interactive content"@en .

html:LabelableElement a owl:Class ;
    dct:conformsTo section:4.10.2 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:ContentCategory ;
    skos:definition "Some elements, not all of them form-associated, are categorized as labelable elements. These are elements that can be associated with a label element."@en ;
    skos:prefLabel "labelable element"@en .

html:Legend a owl:Class ;
    dct:conformsTo section:4.10.16 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:NormalElement ;
    skos:definition "The <legend> HTML element represents a caption for the content of its parent <fieldset>."@en ;
    skos:prefLabel "the legend element"@en ;
    html:tag "legend" .

html:ListedElement a owl:Class ;
    dct:conformsTo section:4.10.2 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:FormAssociatedElement ;
    skos:definition "Denotes elements that are listed in the form.elements and fieldset.elements APIs. These elements also have a form content attribute, and a matching form IDL attribute, that allow authors to specify an explicit form owner."@en ;
    skos:prefLabel "listed element"@en .

html:MetadataContent a owl:Class ;
    dct:conformsTo section:3.2.5.2.1 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:ContentCategory ;
    owl:unionOf ( html:Base html:Link html:Meta html:Noscript html:Script html:Style html:Template html:Title ) ;
    skos:definition "Metadata content is content that sets up the presentation or behavior of the rest of the content, or that sets up the relationship of the document with other documents, or that conveys other \"out of band\" information."@en ;
    skos:prefLabel "Metadata content"@en .

html:PalpableContent a owl:Class ;
    dct:conformsTo section:3.2.5.2.8 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:ContentCategory ;
    owl:unionOf ( [ owl:intersectionOf ( html:Audio [ a owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty html:controls ] ) ] [ owl:intersectionOf ( html:Input [ owl:complementOf [ a owl:Restriction ;
                                    owl:hasValue "hidden" ;
                                    owl:onProperty html:type ] ] ) ] [ owl:intersectionOf ( html:Menu [ a owl:Restriction ;
                            owl:onProperty rdfs:member ;
                            owl:someValuesFrom html:Li ] ) ] [ owl:intersectionOf ( html:Ol [ a owl:Restriction ;
                            owl:onProperty rdfs:member ;
                            owl:someValuesFrom html:Li ] ) ] [ owl:intersectionOf ( html:Ul [ a owl:Restriction ;
                            owl:onProperty rdfs:member ;
                            owl:someValuesFrom html:Li ] ) ] [ owl:intersectionOf ( html:Dl [ owl:intersectionOf ( [ a owl:Restriction ;
                                        owl:onProperty rdfs:member ;
                                        owl:someValuesFrom html:Dt ] [ a owl:Restriction ;
                                        owl:onProperty rdfs:member ;
                                        owl:someValuesFrom html:Dd ] ) ] ) ] html:A html:Abbr html:Address html:Article html:Aside html:B html:Bdi html:Bdo html:Blockquote html:Button html:Canvas html:Cite html:Code html:Data html:Del html:Details html:Dfn html:Div html:Em html:Embed html:Fieldset html:Figure html:Footer html:Form html:H1 html:H2 html:H3 html:H4 html:H5 html:H6 html:Header html:Hgroup html:I html:Iframe html:Img html:Ins html:Kbd html:Label html:Main html:Map html:Mark html:MathML html:Meter html:Nav html:Object html:Output html:P html:Picture html:Pre html:Progress html:Q html:Ruby html:S html:Samp html:Search html:Section html:Select html:Small html:Span html:Strong html:Sub html:Sup html:SVG html:Table html:Textarea html:Time html:U html:Var html:Video html:CustomElement html:Text ) ;
    skos:definition "As a general rule, elements whose content model allows any flow content or phrasing content should have at least one node in its contents that is palpable content and that does not have the hidden attribute specified."@en ;
    skos:note "Palpable content makes an element non-empty by providing either some descendant non-empty text, or else something users can hear (audio elements) or view (video, img, or canvas elements) or otherwise interact with (for example, interactive form controls)." ;
    skos:prefLabel "Palpable content"@en .

html:PhrasingContent a owl:Class ;
    dct:conformsTo section:3.2.5.2.5 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:ContentCategory ;
    owl:unionOf ( [ owl:intersectionOf ( html:Area [ a owl:Restriction ;
                            owl:onProperty [ owl:inverseOf rdfs:member ] ;
                            owl:someValuesFrom html:Map ] ) ] [ owl:intersectionOf ( html:Meta [ a owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty html:itemprop ] ) ] [ owl:unionOf ( [ owl:intersectionOf ( html:Link [ a owl:Restriction ;
                                        owl:cardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onProperty html:itemprop ] ) ] [ owl:intersectionOf ( html:Link [ owl:unionOf ( [ a owl:Restriction ;
                                                    owl:hasValue "dns-prefetch" ;
                                                    owl:onProperty html:rel ] [ a owl:Restriction ;
                                                    owl:hasValue "modulepreload" ;
                                                    owl:onProperty html:rel ] [ a owl:Restriction ;
                                                    owl:hasValue "pingback" ;
                                                    owl:onProperty html:rel ] [ a owl:Restriction ;
                                                    owl:hasValue "preconnect" ;
                                                    owl:onProperty html:rel ] [ a owl:Restriction ;
                                                    owl:hasValue "prefetch" ;
                                                    owl:onProperty html:rel ] [ a owl:Restriction ;
                                                    owl:hasValue "preload" ;
                                                    owl:onProperty html:rel ] [ a owl:Restriction ;
                                                    owl:hasValue "stylesheet" ;
                                                    owl:onProperty html:rel ] ) ] ) ] ) ] html:A html:Abbr html:Audio html:B html:Bdi html:Bdo html:Br html:Button html:Canvas html:Cite html:Code html:Data html:Datalist html:Del html:Dfn html:Em html:Embed html:I html:Iframe html:Img html:Input html:Ins html:Kbd html:Label html:Map html:Mark html:MathML html:Meter html:Noscript html:Object html:Output html:Picture html:Progress html:Q html:Ruby html:S html:Samp html:Script html:Select html:Slot html:Small html:Span html:Strong html:Sub html:Sup html:SVG html:Template html:Textarea html:Time html:U html:Var html:Video html:Wbr html:CustomElement html:Text ) ;
    skos:definition "Phrasing content is the text of the document, as well as elements that mark up that text at the intra-paragraph level. Runs of phrasing content form paragraphs."@en ;
    skos:prefLabel "Phrasing content"@en .

html:ResettableElement a owl:Class ;
    dct:conformsTo section:4.10.2 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:FormAssociatedElement ;
    skos:definition "Denotes elements that can be affected when a form element is reset."@en ;
    skos:prefLabel "resettable element"@en .

html:Row a owl:Class ;
    dct:conformsTo section:4.9.8 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:NormalElement ;
    skos:definition "The <tr> element represents a row of cells in a table."@en ;
    skos:prefLabel "the tr element"@en ;
    html:tag "tr" .

html:Rp a owl:Class ;
    dct:conformsTo section:4.5.12 ;
