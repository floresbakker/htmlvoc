  shp:SerializableElement
    a sh:NodeShape;
    sh:rule rule:Serialize_HTML_fragment_HTML_Element;
    sh:target target:SerializableElement;
    skos:prefLabel 'Serializable element shape'@en.

  target:SerializableElement
    a sh:SPARQLTarget;
    sh:select '''prefix dom: <https://data.rijksfinancien.nl/dom/model/def/>
prefix html: <https://data.rijksfinancien.nl/html/model/def/>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>

# Select the HTML element for which there does not yet exist an HTML
# fragment, with the additional requirement that each and any
# underlying child element contains an HTML fragment.
select $this {
  # Any DOM element (`dom:Element`) that is not known to be a void element
  # (html:VoidElement).
  $this a/rdfs:subClassOf* dom:Element.
  filter not exists {
    $this a/rdfs:subClassOf html:VoidElement.
  }
  # The HTML element must not have an HTML fragment yet.
  filter not exists {
    $this html:fragment [].
  }
  # The HTML element may not contain one or more child elements that lack an
  # HTML fragment.
  minus {
    $this ?member ?childElement.
    ?childElement a/rdfs:subClassOf* dom:Element.
    filter not exists {
      ?childElement html:fragment [].
    }
  }
}'''.

  rule:Serialize_HTML_fragment_HTML_Element
    a sh:SPARQLRule;
    rdfs:comment 'A rule that creates an HTML fragment for an HTML element.'@en;
    skos:prefLabel 'HTML element fragment serialisation rule'@en;
    sh:construct '''prefix html: <https://data.rijksfinancien.nl/html/model/def/>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

construct {

  # create the HTML-fragment for the HTML-element to be serialized.
  $this html:fragment ?fragment.
} where {

  # Get the HTML tag of the HTML-element to be serialized.
  $this a/html:tag ?tag.

  # Get the (possibly empty) sequence of HTML attributes, if there are any.
  {
    select (group_concat(distinct ?attribute;separator=' ') as ?attributes) {
      $this ?attribute0 ?value.
      ?attribute0 html:key ?key.
      bind(concat(?key,'="',str(?value),'"') as ?attribute)
    }
  }

  # Get the (possibly empty) sequence of child element fragments, if there are any.
  {
    select (group_concat(str(?fragment);separator='') as ?fragments) {
      {
        select ?member ?fragment {
          $this ?member ?element.
          ?element html:fragment ?fragment.
          filter(strstarts(str(?member),concat(str(rdf:),'_')))
        }
        order by xsd:integer(strafter(str(?member),concat(str(rdf:),'_')))
      }
    }
  }

  # Create the HTML fragment of this HTML element by combining everything retrieved above.
  bind(strdt(
    concat(
      # open tag and include any attributes if there are any
      '<',?tag,if(?attributes='','',concat(' ',?attributes)),'>',
      # retrieved html content of children html elements
      ?fragments,
      # close tag and cast as HTML snippet
      '</',?tag,'>'),xsd:string) as ?fragment)
}'''.

# Void element

  shp:VoidElement
    a sh:NodeShape;
    skos:prefLabel 'Void element shape'@en;
    sh:rule rule:Serialize_HTML_fragment_Void_Element;
    sh:target target:VoidElement.

  target:VoidElement
    a sh:SPARQLTarget;
    sh:select '''prefix html: <https://data.rijksfinancien.nl/html/model/def/>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
select $this {
  $this a/rdfs:subClassOf* html:VoidElement.
  filter not exists {
    $this html:fragment [].
  }
}'''.

  html:VoidElement
    a owl:Class;
    rdfs:subClassOf dom:Element;
    skos:definition 'A DOM element whose content model never allows it to have contents under any circumstances. Void elements can have attributes.'@en;
    skos:example
      '<br>'@en,
      '<hr>'@en;
    skos:prefLabel 'Void element'@en.

  rule:Serialize_HTML_fragment_Void_Element
    a sh:SPARQLRule;
    rdfs:comment 'A rule that creates an HTML fragment for an HTML Void element.'@en;
    skos:prefLabel 'HTML void element fragment serialisation rule'@en;
    sh:construct '''prefix html: <https://data.rijksfinancien.nl/html/model/def/>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

construct {

  # create the HTML-fragment for the HTML-element to be serialized.
  $this html:fragment ?fragment.
} where {

  # Get the HTML tag of the HTML-element to be serialized.
  $this a/html:tag ?tag.

  # Get the (possibly empty) sequence of HTML attributes, if there are any. Void elements can have attributes.
  {
    select (group_concat(distinct ?attribute;separator=' ') as ?attributes) {
      $this ?attribute0 ?value.
      ?attribute0 html:key ?key.
      bind(concat(?key,'="',str(?value),'"') as ?attribute)
    }
  }

  # Void elements do not have content or children so no need to fetch them.

  # Create the HTML fragment of this HTML element by combining everything retrieved above.
  bind(strdt(
    concat(
      # open tag and include any attributes if there are any
      '<',?tag,if(?attributes='','',concat(' ',?attributes)),'>'
      ),xsd:string) as ?fragment)
}'''.

# Text element

  shp:TextElement
    a sh:NodeShape;
    skos:prefLabel 'Text element shape'@en;
    # no rule need as the text value of the text element should be encoded as html:fragment from the start
    sh:targetClass html:TextElement.

  html:TextElement
    a owl:Class;
    rdfs:subClassOf dom:Element;
    skos:definition 'A DOM element that has textual content and that does not contain any HTML vocabulary.'@en;
    skos:example
      'A numeric value that appears in a table cell.'@en,
      'A natural language sentence that appears in a table caption.'@en;
    skos:prefLabel 'Text element'@en.


