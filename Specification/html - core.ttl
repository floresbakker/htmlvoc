  # HTML conceptual model
  
  concept:html rdf:type skos:ConceptScheme;
    dct:subject html: ;
    skos:prefLabel 'concept scheme of the HTML core vocabulary'@en;
    skos:definition 'The concept scheme of the HTML core vocabulary contains concepts that define aspects of the HTML core vocabulary. It includes concepts that refer to classes, properties, SHACL shapes or other resources, that make up the core of the HTML vocabulary.'@en;
    rdfs:isDefinedBy html:. 

  concept:document rdf:type skos:Concept;
    skos:prefLabel 'HTML document';
    skos:definition 'An HTML document consists of a tree of elements and text. The HTML specification defines a set of elements that can be used in HTML, along with rules about the ways in which the elements can be nested. Elements can have attributes, which control how the elements work. HTML user agents (e.g., web browsers) parse the markup of HTML, turning it into a DOM (Document Object Model) tree. A DOM tree is an in-memory representation of a document. DOM trees contain several kinds of nodes, in particular a DocumentType node, Element nodes, Text nodes, Comment nodes, and in some cases ProcessingInstruction nodes.'@en;
    skos:inScheme concept:html;
    dct:subject html:Document,
	              html:DocumentType,
                html:Element,
                html:Attribute,
                html:Text,
                html:Comment,
                html:ProcessingInstruction;
    rdfs:isDefinedBy html:.

  concept:contentCategory rdf:type skos:Concept;
    skos:prefLabel 'content category';
    skos:definition 'Content category as defined in the HTML core vocabulary'@en;
    skos:inScheme concept:html;             
    dct:subject html:MetadataContent,
                html:FlowContent,
                html:SectioningContent,
                html:HeadingContent,
                html:PhrasingContent,
                html:EmbeddedContent,     
                html:InteractiveContent,
                html:PalpableContent,
                html:ScriptSupportingElement,
                html:FormAssociatedElement,
                html:LabelableElement;
    rdfs:isDefinedBy html:.

  concept:elementType rdf:type skos:Concept;
    skos:prefLabel 'element type';
    skos:definition 'Element type as defined in the HTML core vocabulary'@en;
    skos:inScheme concept:html;             
    dct:subject html:NormalElement,
                html:RawTextElement,
                html:EscapableRawTextElement,
                html:ForeignElement,
                html:VoidElement,
                html:CustomElement;
    rdfs:isDefinedBy html:.

  # HTML - living standard - 3.2 Elements

  # HTML element (<TAG>…</TAG>)

  html:Element
    a owl:Class;
    dct:conformsTo section:3.2;
    rdfs:subClassOf dom:Element;
    skos:definition 'An HTML element in the Document Object Model (DOM) represents a thing; that is, it has intrinsic meaning, also known as semantics. An element consists of an HTML start tag and an HTML end tag and has value content. An HTML start tag consists of a "smaller than" character ("<") and a tag name, followed by a "greater than" character (">"). An HTML end tag consists of a "smaller than" character ("<"), a slash ("/") and a tag name, followed by a "greater than" character (">"). The value content of an element can be arbitrarily complex.'@en;
    skos:prefLabel 'HTML DOM element'@en;
    rdfs:isDefinedBy html:.

  html:NormalElement
    a owl:Class;
    dct:conformsTo section:13.1.2-NormalElements;
    rdfs:subClassOf html:Element;
    skos:definition 'Exclusionary definition: the elements that are neither (1) void elements, (2) the template element, (3) raw text elements, (4) escapable raw text elements, nor (5) foreign elements.'@en;
    skos:prefLabel 'normal element'@en;
    rdfs:isDefinedBy html:.

  html:RawTextElement
    a owl:Class;
    dct:conformsTo section:13.1.2-RawTextElements;
    rdfs:subClassOf html:Element;
    skos:definition 'HTML element where the text inside is treated as raw text and not parsed as HTML.'@en;
    skos:prefLabel 'raw text element'@en;
    rdfs:isDefinedBy html:.

  html:EscapableRawTextElement
    a owl:Class;
    dct:conformsTo section:13.1.2-EscapableRawTextElements;
    rdfs:subClassOf html:Element;
    skos:definition 'HTML element where the text inside is treated as raw text and not parsed as HTML, but character references can still be escaped within their text content.'@en;
    skos:prefLabel 'escapable raw text element'@en;
    rdfs:isDefinedBy html:.

  html:ForeignElement
    a owl:Class;
    dct:conformsTo section:13.1.2-ForeignElements;
    rdfs:subClassOf html:Element;
    skos:definition 'HTML element from a namespace other than the HTML namespace.'@en;
    skos:prefLabel 'foreign element'@en;
    rdfs:isDefinedBy html:.

  html:VoidElement
    a owl:Class;
    rdfs:subClassOf html:Element;
    skos:definition 'A HTML element whose content model never allows it to have contents under any circumstances. Void elements can have attributes.'@en;
    skos:example
      '<br>'@en,
      '<hr>'@en;
    skos:prefLabel 'void element'@en;
    rdfs:isDefinedBy html:.

  # Custom element

  html:CustomElement
    a owl:Class;
    rdfs:subClassOf html:Element;
    dct:conformsTo section:4.13.3;
    skos:definition 'A custom element is an element that is custom. Informally, this means that its constructor and prototype are defined by the author, instead of by the user agent. This author-supplied constructor function is called the custom element constructor. Two distinct types of custom elements can be defined: An autonomous custom element, which is defined with no extends option. These types of custom elements have a local name equal to their defined name. A customized built-in element, which is defined with an extends option. These types of custom elements have a local name equal to the value passed in their extends option, and their defined name is used as the value of the is attribute, which therefore must be a valid custom element name.'@en;
    skos:prefLabel 'custom HTML element'@en;
    skos:example """

    ex:someFlagIconElement rdf:type ex:FlagIcon.
    ex:FlagIcon rdf:type html:CustomElement;
                html:tag 'flag-icon';
                html:extends html:Element;
                rdfs:isDefinedBy ex:.

    """;
    rdfs:isDefinedBy html:.

  html:AutonomousCustomElement
    a owl:Class;
    rdfs:subClassOf html:CustomElement;
    owl:equivalentClass [rdf:type owl:Restriction;
                         owl:someValuesFrom [owl:intersectionOf (html:Element [owl:complementOf [owl:unionOf (html:NormalElement html:RawTextElement html:EscapableRawTextElement html:ForeignElement html:VoidElement html:CustomizedBuiltInElement)]])];
                         owl:onProperty html:extends];
    dct:conformsTo section:4.13.3;                         
    skos:definition 'An autonomous custom element is a custom HTML element that is defined with no extends option. These types of custom elements have a local name equal to their defined name.'@en;
    skos:prefLabel 'autonomous custom element'@en;
    rdfs:isDefinedBy html:.

  html:CustomizedBuiltInElement
    a owl:Class;
    rdfs:subClassOf html:CustomElement;
    owl:equivalentClass [rdf:type owl:Restriction;
                         owl:someValuesFrom [owl:unionOf (html:NormalElement html:RawTextElement html:EscapableRawTextElement html:ForeignElement html:VoidElement)];
                         owl:onProperty html:extends];
    dct:conformsTo section:4.13.3;                         
    skos:definition 'A customized built-in element is a custom HTML element that is defined with an extends option. These types of custom elements have a local name equal to the value passed in their extends option, and their defined name is used as the value of the is attribute, which therefore must be a valid custom element name.'@en;
    skos:prefLabel 'customized built-in element'@en;
    rdfs:isDefinedBy html:.                         

  # CDATA section

  html:CDATASection
    a owl:Class;
    rdfs:subClassOf dom:CDATASection;
    dct:conformsTo section:13.1.5;
    skos:definition 'A DOM element with textual content that contain characters that would otherwise be treated as markup. A CDATA section is typically used to include code snippets, scripts, or other data within an HTML document without having to worry about escaping special characters. In HTML, a CDATA section is denoted by enclosing the block of text within <![CDATA[ and ]]> tags. Anything contained within these tags is treated as raw character data and is not parsed as markup.'@en;
    skos:example'''
<p>You can add a string to a number, but this stringifies the number:</p>
<math>
 <ms><![CDATA[x<y]]></ms>
 <mo>+</mo>
 <mn>3</mn>
 <mo>=</mo>
 <ms><![CDATA[x<y3]]></ms>
</math>'''@en;
    skos:prefLabel 'CDATA section'@en;
    rdfs:isDefinedBy html:.

  # Comment

  html:Comment
    a owl:Class;
    rdfs:subClassOf dom:Comment;
    dct:conformsTo section:13.1.6;
    skos:definition 'A comment is a markup construct used to insert comments within the HTML code. Comments are not displayed in the web browser, but they can be viewed in the HTML source code. Comments are typically used to add notes, descriptions, or explanations to the HTML code for the benefit of developers, without affecting the rendered output in the browser. Comments are denoted by enclosing the comment text within <!-- and --> tags. Anything contained within these tags is treated as a comment and is ignored by the web browser during rendering.'@en;
    skos:example'''<!-- Write here your comment -->'''@en;
    skos:prefLabel 'comment element'@en;
    rdfs:isDefinedBy html:.

  # Text node

  html:Text
    a owl:Class;
    rdfs:subClassOf dom:Text;
    dct:conformsTo section:13.1.3;
    skos:definition 'Textual content that does not contain any HTML Core Vocabulary.'@en;
    skos:example
      'A numeric value that appears in a table cell.'@en,
      'A natural language sentence that appears in a table caption.'@en;
    skos:prefLabel 'text'@en;
    rdfs:isDefinedBy html:.

  # Content category

  html:ContentCategory
    rdf:type owl:Class;
    dct:conformsTo section:3.2.5.2;
    skos:definition 'Each element in HTML falls into zero or more categories that group elements with similar characteristics together.'@en;
    skos:prefLabel 'content category'@en;
    skos:note 'Some elements also fall into other categories, which are defined in other parts of this specification.';
    rdfs:isDefinedBy html:.

  html:MetadataContent
    rdf:type owl:Class;
    rdfs:subClassOf html:ContentCategory;
    owl:unionOf (html:Base html:Link html:Meta html:Noscript html:Script html:Style html:Template html:Title);
    dct:conformsTo section:3.2.5.2.1;
    skos:definition 'Metadata content is content that sets up the presentation or behavior of the rest of the content, or that sets up the relationship of the document with other documents, or that conveys other "out of band" information.'@en;
    skos:prefLabel 'Metadata content'@en;
    rdfs:isDefinedBy html:.

  html:FlowContent
    rdf:type owl:Class;
    rdfs:subClassOf html:ContentCategory;
    owl:unionOf (
                  [owl:intersectionOf (html:Area [rdf:type owl:Restriction;
                                                    owl:someValuesFrom html:Map ;
                                                    owl:onProperty [owl:inverseOf rdfs:member]
                                                   ] 
                                         )]
                  [owl:unionOf(
                                    [owl:intersectionOf (html:Link [rdf:type owl:Restriction ; 
                                                                    owl:cardinality "1"^^xsd:nonNegativeInteger ; 
                                                                    owl:onProperty  html:itemprop]
                                                                )] 

                                    [owl:intersectionOf (html:Link [owl:unionOf(

                                                                                 [rdf:type owl:Restriction;
                                                                                  owl:onProperty html:rel;
                                                                                  owl:hasValue "dns-prefetch"^^xsd:string]

                                                                                 [rdf:type owl:Restriction;
                                                                                  owl:onProperty html:rel;
                                                                                  owl:hasValue "modulepreload"^^xsd:string]

                                                                                 [rdf:type owl:Restriction;
                                                                                  owl:onProperty html:rel;
                                                                                  owl:hasValue "pingback"^^xsd:string]

                                                                                 [rdf:type owl:Restriction;
                                                                                  owl:onProperty html:rel;
                                                                                  owl:hasValue "preconnect"^^xsd:string]

                                                                                 [rdf:type owl:Restriction;
                                                                                  owl:onProperty html:rel;
                                                                                  owl:hasValue "prefetch"^^xsd:string]

                                                                                 [rdf:type owl:Restriction;
                                                                                  owl:onProperty html:rel;
                                                                                  owl:hasValue "preload"^^xsd:string]

                                                                                 [rdf:type owl:Restriction;
                                                                                  owl:onProperty html:rel;
                                                                                  owl:hasValue "stylesheet"^^xsd:string]
                                                                                 )]   
                                                                )]
                                )] 
                  [owl:intersectionOf (html:Meta [rdf:type owl:Restriction ; 
                                                    owl:cardinality "1"^^xsd:nonNegativeInteger ; 
                                                    owl:onProperty  html:itemprop]
                                                )]    
                  [owl:intersectionOf (html:Main [owl:unionOf (
                                                                   [rdf:type owl:Restriction;
                                                                    owl:someValuesFrom html:Html ;
                                                                    owl:onProperty [owl:inverseOf rdfs:member]
                                                                    ]

                                                                   [rdf:type owl:Restriction;
                                                                    owl:someValuesFrom html:Body ;
                                                                    owl:onProperty [owl:inverseOf rdfs:member]
                                                                    ]
                                                                    
                                                                   [rdf:type owl:Restriction;
                                                                    owl:someValuesFrom html:Div ;
