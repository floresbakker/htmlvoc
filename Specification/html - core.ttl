  function:getDocumentTypeFragment
    a sh:SPARQLFunction ;
    skos:prefLabel  "the getDocumentTypeFragment() function"@en;    
    skos:definition "Returns a HTML fragment for a HTML DocumentType in a HTML document."@en ;
    sh:parameter [
        sh:path function:doctype ;
